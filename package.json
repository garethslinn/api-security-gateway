{
  "name": "api-security-gateway",
  "version": "1.0.0",
  "description": "A security gateway for APIs with rate limiting, IP whitelisting, and injection prevention.",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "watch": "babel src --out-dir dist --watch",
    "start": "node dist/index.js",
    "test": "jest",
    "minify": "uglifyjs dist/index.js -o dist/index.js",
    "dist": "npm run build && npm run minify",
    "prepublishOnly": "npm run dist"
  },
  "keywords": [
    "api",
    "rate limiting",
    "ip whitelisting",
    "injection prevention",
    "backend",
    "cache",
    "caching",
    "cert",
    "certificate",
    "clickjack",
    "connect",
    "content",
    "content-security-policy",
    "csp",
    "cross-origin-embedder-policy",
    "cross-origin-opener-policy",
    "cross-origin-resource-policy",
    "csrf",
    "express",
    "frame",
    "headers",
    "hpkp",
    "hsts",
    "https",
    "key",
    "mimetype",
    "nocache",
    "noopen",
    "origin-agent-cluster",
    "pinni",
    "policy",
    "powered-by",
    "referrer-policy",
    "security",
    "sri",
    "ssl",
    "strict-transport-security",
    "x-content-type-options",
    "x-dns-prefetch-control",
    "x-download-options",
    "x-frame-options",
    "x-permitted-cross-domain-policies",
    "x-powered-by",
    "x-xss-protection",
    "xss"
  ],
  "author": "G.Slinn",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "babel-jest": "^29.7.0",
    "jest": "^27.0.6",
    "supertest": "^6.1.3",
    "uglify-js": "^3.18.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/garethslinn/api-security-gateway.git"
  }
}
